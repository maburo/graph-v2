{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "DOM",
      "ES2015", 
      "ES2016",
      "ES2019"
    ],
    "jsx": "react"
  },
  "include": [
    "custom.d.ts",
    "./src/**/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}